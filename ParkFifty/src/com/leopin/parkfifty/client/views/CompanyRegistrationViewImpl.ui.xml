<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.leopin.parkfifty.client.ui">

	<ui:with field='res'
		type='com.leopin.parkfifty.client.resources.ParkFiftyResources' />

	<g:FlowPanel styleName="{res.style.column}">
		<g:FlowPanel>
			<g:HTML styleName="{res.style.title}"><span class="{res.style.titleSection}">Sign up for an Account, Continued...</span></g:HTML>
			<g:FlowPanel>
				<c:CompanyWidget styleName="{res.style.floatL}" />
				<c:CompanyOwnerWidget styleName="{res.style.floatL}" />
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.style.navbar}">
				<g:Button addStyleNames="{res.style.button}">Save</g:Button>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>


</ui:UiBinder> 