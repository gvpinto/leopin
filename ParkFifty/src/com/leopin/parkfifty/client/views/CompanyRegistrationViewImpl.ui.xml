<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.leopin.parkfifty.client.ui">

	<ui:with field='res'
		type='com.leopin.parkfifty.client.resources.AppStyles' />
		
		

	<g:HTMLPanel> styleName="{res.style.column}">
		
			<g:HTML styleName="{res.style.title}"><span class="{res.style.titleSection}">Sign up for an Account, Continued...</span></g:HTML>
			<g:FlowPanel styleName="{res.style.center}">
				<c:CompanyWidget ui:field="uiCompany" styleName="{res.style.floatL}" />
				<c:CompanyOwnerWidget ui:field="uiCompanyOwner" styleName="{res.style.floatL}" />
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.style.navbar}">
				<g:Button ui:field="uiSubmit" addStyleNames="{res.style.button}">Submit</g:Button>
				<g:Button ui:field="uiCancel" addStyleNames="{res.style.button}">Cancel</g:Button>
			</g:FlowPanel>

	</g:HTMLPanel>


</ui:UiBinder> 