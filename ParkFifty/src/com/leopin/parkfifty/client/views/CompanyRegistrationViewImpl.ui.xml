<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.leopin.parkfifty.client.ui">

	<ui:with field='res'
		type='com.leopin.parkfifty.client.resources.AppStyles' />
		
		

	<g:HTMLPanel>
		
		<fieldset class="{res.style.groupTitle}">
		
			<legend ui:field="uiLegend"/>

			<g:FlowPanel>
				<c:CompanyWidget ui:field="uiCompany" addStyleNames="{res.style.floatL}" />
				<c:CompanyOwnerWidget ui:field="uiCompanyOwner" addStyleNames="{res.style.floatL}" />
			</g:FlowPanel>
			
			<g:FlowPanel styleName="{res.style.navbar}">
				<g:Button ui:field="uiSubmit" addStyleNames="{res.style.button},{res.style.yes}">Submit</g:Button>  
				<!-- <g:Button ui:field="uiSubmit" addStyleNames="{res.style.button}">Submit</g:Button> -->
				<!-- <g:Image resource="{res.resources.yes}"/> -->
				<g:Button ui:field="uiCancel" addStyleNames="{res.style.button} {res.style.no}">Cancel</g:Button>
			</g:FlowPanel>

		</fieldset>
		
	</g:HTMLPanel>


</ui:UiBinder> 