<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='com.leopin.parkfifty.client.resources.ParkFiftyResources'/>
	<g:HTMLPanel styleName="{res.companyWidget}">
		<g:FlowPanel>
			<g:Label>Sign Up</g:Label>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label>Company Name:</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Website URL:</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Email Address:</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Phone Number:</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Other Phone Number:</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Fax</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Fax</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox></g:TextBox>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:Button ui:field="uiContinue">Continue</g:Button>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 