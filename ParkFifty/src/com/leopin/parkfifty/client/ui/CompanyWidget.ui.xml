<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.leopin.parkfifty.client.ui">

	<ui:with field='res'
		type='com.leopin.parkfifty.client.resources.AppStyles' />

	<g:HTMLPanel styleName="{res.style.labelColor}">

		<c:TextBoxCombo ui:field="uiName" name="uiName"
			 visibleLength="250" maxLength="100"/>

		<c:TextBoxCombo ui:field="uiUrl" name="uiUrl"
			visibleLength="250" maxLength="100" />

		<c:TextBoxCombo ui:field="uiEmail" name="uiEmail"
			visibleLength="250" maxLength="300" />

		<c:TextBoxCombo ui:field="uiPriPhone" name="uiPriPhone"
			visibleLength="250" maxLength="15" />

		<c:TextBoxCombo ui:field="uiSecPhone" name="uiSecPhone"
			visibleLength="250" maxLength="15" />
			
		<c:TextBoxCombo ui:field="uiFax" name="uiFax"
			visibleLength="250" maxLength="15" />

	</g:HTMLPanel>

</ui:UiBinder> 