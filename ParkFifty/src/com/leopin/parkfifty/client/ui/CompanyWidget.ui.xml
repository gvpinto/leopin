<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.leopin.parkfifty.client.ui">
	<ui:with field='res' type='com.leopin.parkfifty.client.resources.ParkFiftyResources'/>
	<g:HTMLPanel styleName="{res.style.companyWidget}">
		<g:FlowPanel>
			<g:Label>Sign Up</g:Label>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label>Company Name:</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiName" name="uiName" maxLength="100" visibleLength="30"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Website URL:</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiUrl" name="uiUrl" visibleLength="30" maxLength="100"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Email Address:</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiEmail"  name="uiEmail" visibleLength="30" maxLength="300"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Phone Number:</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiPriPhone" name="uiPriPhone" visibleLength="30" maxLength="15"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Secondary Phone Number:">Other Phone Number:</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiSecPhone"  name="uiSecPhone" visibleLength="30" maxLength="15"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Fax</g:Label>
					</g:customCell>
					<g:customCell>
						<c:TextBoxCombo ui:field="uiFax" name="uiFax" visibleLength="30" maxLength="15"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:Button ui:field="uiContinue">Continue</g:Button>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 