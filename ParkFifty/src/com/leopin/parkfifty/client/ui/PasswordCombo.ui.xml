<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='com.leopin.parkfifty.client.resources.AppStyles'/>

	<g:HTMLPanel styleName="{res.style.textBoxCombo}">
		<label ui:field="uiLabel"></label>
		<g:PasswordTextBox ui:field="uiTextBox" styleName="{res.style.textBox}" />
		<g:InlineLabel ui:field="uiHelpText"></g:InlineLabel>
	</g:HTMLPanel>
	
</ui:UiBinder> 