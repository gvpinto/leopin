<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.leopin.parkfifty.client.ui">
	<ui:with field='res' type='com.leopin.parkfifty.client.resources.ParkFiftyResources'/>

	<g:FlowPanel>
		<g:Grid ui:field="uiGrid">
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label wordWrap="false">User ID:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserUsername" name="uiUserUsername" maxLength="12" visibleLength="25"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Password:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:PasswordCombo ui:field="uiUserPassword" name="uiUserPassword" visibleLength="25" maxLength="20"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Title:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserTitle"  name="uiUserTitle" visibleLength="4" maxLength="4"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">First Name:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserFirstName" name="uiUserFirstName" visibleLength="25" maxLength="40"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Middle Initial:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserMiddleInitial" name="uiUserMiddleInitial" visibleLength="1" maxLength="1"/>
				</g:customCell>
			</g:row>					
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Last Name:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserLastName" name="uiUserLastName" visibleLength="25" maxLength="40"/>
				</g:customCell>
			</g:row>		
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Suffix:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserSuffix" name="uiUserSuffix" visibleLength="3" maxLength="3"/>
				</g:customCell>
			</g:row>		
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label text="Primary Phone Number:" horizontalAlignment="ALIGN_RIGHT">Other Phone Number:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserPriPhone"  name="uiUserPriPhone" visibleLength="25" maxLength="15"/>
				</g:customCell>
			</g:row>				
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label text="Secondary Phone Number:" horizontalAlignment="ALIGN_RIGHT">Other Phone Number:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserSecPhone"  name="uiUserSecPhone" visibleLength="25" maxLength="15"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label text="Fax:" horizontalAlignment="ALIGN_RIGHT">Fax</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserFax" name="uiUserFax" visibleLength="25" maxLength="15"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell  styleName="{res.style.labelCell}">
					<g:Label horizontalAlignment="ALIGN_RIGHT">Email Address:</g:Label>
				</g:customCell>
				<g:customCell  styleName="{res.style.fieldCell}">
					<c:TextBoxCombo ui:field="uiUserEmail"  name="uiUserEmail" visibleLength="25" maxLength="300"/>
				</g:customCell>
			</g:row>				
		</g:Grid>
	</g:FlowPanel>

</ui:UiBinder> 