<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.leopin.parkfifty.client.ui">
	<ui:with field='res' type='com.leopin.parkfifty.client.resources.AppStyles'/>


	<g:HTMLPanel styleName="{res.style.labelColor}">

		<c:TextBoxCombo ui:field="uiUsername" name="uiUsername"
			maxLength="12" visibleLength="25" />

		<c:PasswordCombo ui:field="uiPassword" name="uiPassword" 
			visibleLength="25" maxLength="20" />

		<c:TextBoxCombo ui:field="uiUserTitle" name="uiUserTitle"
			visibleLength="4" maxLength="4" />

		<c:TextBoxCombo ui:field="uiUserFirstName" name="uiUserFirstName"
			visibleLength="25" maxLength="40" />

		<c:TextBoxCombo ui:field="uiUserMiddleInitial" name="uiUserMiddleInitial"
			visibleLength="1" maxLength="1" />
			
		<c:TextBoxCombo ui:field="uiUserLastName" name="uiUserLastName"
			visibleLength="25" maxLength="40" />

		<c:TextBoxCombo ui:field="uiUserSuffix" name="uiUserSuffix"
			visibleLength="3" maxLength="3" />

		<c:TextBoxCombo ui:field="uiUserEmail" name="uiUserEmail"
			visibleLength="25" maxLength="300" />

		<c:TextBoxCombo ui:field="uiUserPriPhone" name="uiUserPriPhone"
			visibleLength="25" maxLength="15" />

		<c:TextBoxCombo ui:field="uiUserSecPhone" name="uiUserSecPhone"
			visibleLength="25" maxLength="15" />

		<c:TextBoxCombo ui:field="uiUserFax" name="uiUserFax"
			visibleLength="25" maxLength="15" />

	</g:HTMLPanel>

</ui:UiBinder> 